{
  "name": "Shades",
  "colors": {
    // Add new color enhancements
    "activityBar.background": "#242639",
    "activityBar.foreground": "#12d8f6",
    // Main editor colors - Using a deep, rich background that's easier on the eyes
    "editor.background": "#25273d",
    "editor.foreground": "#E2E8F0",
    "editor.findMatchBackground": "#55f6d650", // Background for the current match
    "editor.findMatchHighlightBackground": "#28f1bf40", // Background for other matches
    "editor.findMatchBorder": "#55f6f1", // Border for the current match
    "editor.findMatchHighlightBorder": "#6cff7380", // Border for other matches

    // Status Bar - Making it more informative with distinct states
    "statusBar.background": "#2D3748",
    "statusBar.foreground": "#9DC6FF",
    // Debugging state - Using vibrant purple to indicate active debugging
    "statusBar.debuggingBackground": "#9F7AEA",
    "statusBar.debuggingForeground": "#FFFFFF",
    // Remote state - Using distinctive blue to show remote connection
    "statusBarItem.remoteBackground": "#3182CE",
    "statusBarItem.remoteForeground": "#FFFFFF",

    // Debug UI - Making debugging more intuitive
    "debugToolBar.background": "#2D3748",
    "debugToolBar.border": "#4A5568",
    // Using subtle but noticeable highlights for stack frames
    "editor.stackFrameHighlightBackground": "#b5ed0a80",
    "editor.focusedStackFrameHighlightBackground": "#9F7AEA40",
    // Making variable states more distinctive
    "debugTokenExpression.name": "#90CDF4",
    "debugTokenExpression.value": "#9AE6B4",
    "debugTokenExpression.string": "#B794F4",
    "debugTokenExpression.boolean": "#F687B3",
    "debugTokenExpression.number": "#F6AD55",
    "debugTokenExpression.error": "#FC8181",

    // Debug Console - Using intuitive colors for different message types
    "debugConsole.infoForeground": "#90CDF4",
    "debugConsole.warningForeground": "#F6E05E",
    "debugConsole.errorForeground": "#FC8181",
    "debugConsole.sourceForeground": "#B2F5EA",

    // Enhanced Sidebar Selection
    "list.activeSelectionBackground": "#3182CE30", // Semi-transparent blue background
    "list.activeSelectionForeground": "#63B3ED", // Bright blue text for selected item
    "list.activeSelectionIconForeground": "#63B3ED", // Matching icon color
    "list.inactiveSelectionBackground": "#2D374850", // Dimmer background for inactive selection
    "list.inactiveSelectionForeground": "#90CDF4", // Slightly dimmer text for inactive selection

    // Adding distinct borders for selected items
    "list.highlightForeground": "#63B3ED", // Matching text highlight
    "list.focusHighlightForeground": "#63B3ED", // Matching focus highlight
    "list.focusOutline": "#3182CE", // Outline for keyboard focus
    "list.focusAndSelectionOutline": "#3182CE", // Combined focus and selection

    // Hover states
    "list.hoverBackground": "#2D374830", // Subtle background on hover
    "list.hoverForeground": "#E2E8F0", // Bright text on hover

    // Making the active item more prominent
    "list.filterMatchBorder": "#3182CE", // Border for search matches
    "list.filterMatchBackground": "#3182CE20", // Background for search matches

    // Basic minimap colors
    "minimap.background": "#082e4d83", // Background of the entire minimap
    "minimap.selectionHighlight": "#82ecffb5", // Color of selected text in minimap

    // Indent guides in tree view
    "tree.indentGuidesStroke": "#b0c9f6d5", // Subtle but visible indent guides

    // Dropdown and selection improvements
    "dropdown.border": "#3182CE40", // Matching border for dropdowns
    "selection.background": "#3182CE40",

    // Panel Colors - Making the bottom panel distinct but harmonious
    "panel.background": "#1A202C",
    "panel.border": "#4A5568",
    "panelTitle.activeBorder": "#3182CE",
    "panelTitle.activeForeground": "#FFFFFF",
    "panelTitle.inactiveForeground": "#A0AEC0",

    // Terminal - Creating a modern, readable terminal experience
    "terminal.background": "#202234",
    "terminal.foreground": "#E2E8F0",
    // ANSI Colors - Using vibrant, distinguishable colors
    "terminal.ansiBlack": "#2D3748",
    "terminal.ansiBlue": "#4299E1",
    "terminal.ansiBrightBlack": "#4A5568",
    "terminal.ansiBrightBlue": "#63B3ED",
    "terminal.ansiBrightCyan": "#76E4F7",
    "terminal.ansiBrightGreen": "#9AE6B4",
    "terminal.ansiBrightMagenta": "#B794F4",
    "terminal.ansiBrightRed": "#FC8181",
    "terminal.ansiBrightWhite": "#FFFFFF",
    "terminal.ansiBrightYellow": "#F6E05E",
    "terminal.ansiCyan": "#0BC5EA",
    "terminal.ansiGreen": "#68D391",
    "terminal.ansiMagenta": "#9F7AEA",
    "terminal.ansiRed": "#F56565",
    "terminal.ansiWhite": "#E2E8F0",
    "terminal.ansiYellow": "#ECC94B",
    "terminal.selectionBackground": "#4A556880",

    // Sidebar - Creating visual hierarchy
    "sideBar.background": "#2c2e44", // 1
    "sideBar.foreground": "#E2E8F0",
    "sideBar.border": "#2D374840",
    "sideBarTitle.foreground": "#90CDF4",
    "sideBarSectionHeader.background": "#2D3748",
    "sideBarSectionHeader.foreground": "#90CDF4",
    "sideBarSectionHeader.border": "#4A556840",

    // Tabs - Making file navigation clearer
    "tab.activeBackground": "#1E1E2E",
    "tab.activeForeground": "#FFFFFF",
    "tab.activeBorderTop": "#3182CE",
    "tab.inactiveBackground": "#1A202C",
    "tab.inactiveForeground": "#A0AEC0",
    "tab.unfocusedActiveBorderTop": "#4A556880",
    "tab.hoverBackground": "#2D374850",

    // Welcome Page - Creating an inviting first impression
    "welcomePage.background": "#1E1E2E",
    // "welcomePage.buttonBackground": "#2D3748",
    // "welcomePage.buttonHoverBackground": "#4A5568",

    // Notifications - Making them noticeable but not distracting
    "notifications.background": "#2D3748",
    "notifications.foreground": "#E2E8F0",
    "notifications.border": "#4A5568",
    "notificationLink.foreground": "#63B3ED",
    "notificationCenterHeader.background": "#1A202C",
    "notificationCenterHeader.foreground": "#E2E8F0",

    // Quick Input (Command Palette) - Making it stand out
    "quickInput.background": "#2D3748",
    "quickInput.foreground": "#E2E8F0",
    "quickInputTitle.background": "#4A5568",
    "pickerGroup.border": "#63B3ED40",
    "pickerGroup.foreground": "#90CDF4",

    // Extensions - Making important actions stand out
    "extensionButton.prominentBackground": "#3182CE",
    "extensionButton.prominentForeground": "#FFFFFF",
    "extensionButton.prominentHoverBackground": "#4299E1"
  },

  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#546E7A"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#EEFFFF"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#ffffff"
